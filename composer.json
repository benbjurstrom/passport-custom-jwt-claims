{
    "name": "benbjurstrom/passport-custom-jwt-claims",
    "description": "Customize JWT claims in Laravel Passport access tokens",
    "type": "laravel",
    "keywords": [
        "jwt",
        "api",
        "laravel",
        "passport",
        "claims",
        "custom",
        "openid connect",
        "openid",
        "oidc"
    ],
    "require": {
        "php": ">=7.1.3",
        "laravel/passport": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Bjurstrom",
            "email": "bbjurstrom@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "BenBjurstrom\\JwtClaims\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BenBjurstrom\\JwtClaims\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "BenBjurstrom\\JwtClaims\\JwtClaimsServiceProvider"
            ]
        }
    }
}
